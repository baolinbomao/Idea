{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["dom", "es5", "es6", "es2016.array.include"],
    "target": "es5",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "library", "local", "temp", "build", "settings"]
}
